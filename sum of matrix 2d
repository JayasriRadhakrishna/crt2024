sum of matrix 2d

#include <stdio.h>
// read m;n matrix and print sum of matrix//
int main()
{
    int m,n;
    scanf("%d%d",&m,&n);
    int a[m][n],i,j;
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        scanf("%d",&a[i][j]);
    }
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        printf("%d ",a[i][j]);
    printf("\n");
    }
    int s=0;
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        s=s+a[i][j];
        printf("%d ",s);
       
    }
    
    
}
